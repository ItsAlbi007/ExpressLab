<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>First Express</title>
</head>
<body>
  <h1>Gym work outs</h1>
  <ul>
    <% gym.forEach(function(g) { %>
      <li>
        <%= g.gym %>
          -
        <%= g.done ? 'done' : 'not done' %>  
      </li>
    <% })  %>
  </ul>
</body>
</html>